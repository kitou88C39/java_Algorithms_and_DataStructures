import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        //入力を受け取る
        int n = sc.nextInt();
        int[ ] a = new int[n];
        for(int i = 0; i < n; i++){
            a[i] = sc.nextInt();
        }
    //答えを保存する変数を用意して適切な初期値で初期化
        int maximum = 1000000;
        int minimum = 1000000;
    }
}
